{
  "name": "todo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "json-server": "^0.15.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "npm test -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "mock_server": "json-server --watch ./db.json --port 3001",
    "prettier": "prettier --write src/**/*.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^1.18.2",
    "sinon": "^7.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/*.js",
      "!./src/{index,serviceWorker}.js"
    ],
    "coverageReporters": [
      "text",
      "html"
    ]
  }
}
